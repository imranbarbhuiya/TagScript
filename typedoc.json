{
	"$schema": "https://typedoc.org/schema.json",
	"cleanOutputDir": true,
	"entryPointStrategy": "packages",
	"entryPoints": ["packages/tagscript/", "packages/tagscript-plugin-discord/"],
	"excludeExternals": true,
	"name": "TagScript",
	"out": "./docs/",
	"tsconfig": "./tsconfig.base.json",
	"disableSources": true,
	"externalLinkPath": "./scripts/externalConfig.js",
	"hideGenerator": true,
	"hideBreadcrumbs": true,
	"hideInPageTOC": true,
	"publicPath": "./tagscript/",
	"plugin": ["typedoc-plugin-markdown", "typedoc-plugin-mdn-links", "typedoc-plugin-resolve-crossmodule-references", "typedoc-plugin-external-link"]
}
